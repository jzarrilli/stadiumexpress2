<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= link_to image_tag("show.png", :border => 0), user %></td>
    <td><%= link_to image_tag("edit.png", :border => 0), edit_user_path(user) %></td>
